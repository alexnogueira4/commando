{
  "name": "rasphome",
  "version": "0.0.1",
  "author": "Alexsander Nogueira",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "npx nodemon",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.js --max-warnings 0",
    "prod": "npm run build && node dist/entry.js",
    "start": "npx nodemon",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "4.17.21",
    "@types/express-promise-router": "^3.0.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "^25.2.3",
    "@types/morgan": "^1.9.0",
    "@types/supertest": "^2.0.9",
    "dns-txt": "^2.0.2",
    "dotenv": "^8.2.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jest": "^26.0.1",
    "mdns": "^2.7.2",
    "mime": "^3.0.0",
    "morgan": "^1.10.0",
    "multicast-dns": "^7.2.5",
    "shelljs": "0.8.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "chai": "^4.2.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "nodemon": "^2.0.20",
    "prettier": "3.2.4",
    "request": "^2.88.2",
    "should": "^13.2.3",
    "ts-node": "7.0.1",
    "typescript": "3.9.10"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
